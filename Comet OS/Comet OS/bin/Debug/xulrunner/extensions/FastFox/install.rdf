<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>smarterwiki@wikiatic.com</em:id>
        <em:version>5.1.3</em:version>
        <em:type>2</em:type>
   
        <!-- Target Application this extension can install into, 
             with minimum and maximum supported versions. --> 
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.0</em:minVersion>
                <em:maxVersion>18.*</em:maxVersion>
            </Description>
        </em:targetApplication>
   
        <!-- Front End MetaData -->
        <em:name>FastestFox</em:name>
        <em:description>View &quot;related articles&quot; box on Wikipedia, search selected text from popup bubble and context menu, search from the Awesomebar, easily switch search engines for a search, and visit bookmarks with keyboard shortcuts!</em:description>
        <em:creator>smarterfox.com</em:creator>
        <em:homepageURL>http://smarterfox.com/</em:homepageURL>
        
        <!-- Optional options -->
        <em:iconURL>chrome://smarterwiki/skin/smarterfox-logo-32x32.png</em:iconURL><!-- 32x32 -->
        <em:optionsURL>chrome://smarterwiki/content/options.xul</em:optionsURL>



